<?xml version='1.0' encoding='utf-8'?>

<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">

<hibernate-configuration>

    <session-factory>
        <property name="connection.driver_class">org.h2.Driver</property>
        <property name="connection.url">jdbc:h2:~/treeDB</property>
        <property name="connection.username">userTree</property>
        <property name="connection.password">pass</property>

        <property name="show_sql">true</property>

        <mapping class="com.example.data.Area"/>
        <mapping class="com.example.data.BusinessTripReadiness"/>
        <mapping class="com.example.data.Certificate"/>
        <mapping class="com.example.data.Contact"/>
        <mapping class="com.example.data.ContactType"/>
        <mapping class="com.example.data.Currency"/>
        <mapping class="com.example.data.District"/>
        <mapping class="com.example.data.DriverLicenseType"/>
        <mapping class="com.example.data.Education"/>
        <mapping class="com.example.data.EducationLevel"/>
        <mapping class="com.example.data.Employer"/>
        <mapping class="com.example.data.Employment"/>
        <mapping class="com.example.data.Experience"/>
        <mapping class="com.example.data.Gender"/>
        <mapping class="com.example.data.HiddenField"/>
        <mapping class="com.example.data.Industry"/>
        <mapping class="com.example.data.Language"/>
        <mapping class="com.example.data.LanguageLevel"/>
        <mapping class="com.example.data.Line"/>
        <mapping class="com.example.data.Metro"/>
        <mapping class="com.example.data.Owner"/>
        <mapping class="com.example.data.Photo"/>
        <mapping class="com.example.data.Platform"/>
        <mapping class="com.example.data.Portfolio"/>
        <mapping class="com.example.data.ProfessionalRole"/>
        <mapping class="com.example.data.Relocation"/>
        <mapping class="com.example.data.RelocationType"/>
        <mapping class="com.example.data.Resume"/>
        <mapping class="com.example.data.ResumeLocale"/>
        <mapping class="com.example.data.Schedule"/>
        <mapping class="com.example.data.Site"/>
        <mapping class="com.example.data.SiteType"/>
        <mapping class="com.example.data.Specialization"/>
        <mapping class="com.example.data.TravelTime"/>
    </session-factory>

</hibernate-configuration>
